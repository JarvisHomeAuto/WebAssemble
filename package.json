{
  "name": "webassemble",
  "version": "0.9.0",
  "description": "A universal webassembly based front-end framework, with a virtual dom for JSX components.",
  "main": "index.js",
  "scripts": {
    "test": "node tests",
    "pack": "npx parcel index.html",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "keywords": [
    "web",
    "assembly",
    "python",
    "rust",
    "js",
    "javascript",
    "ts",
    "typescript",
    "framework",
    "C",
    "C++",
    "toolkit"
  ],
  "author": "Parker Bidigare",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "assemblyscript": "^0.20.16",
    "babel-plugin-closure-elimination": "^1.3.2",
    "babel-plugin-groundskeeper-willie": "^2.0.0",
    "faster.js": "^1.1.1",
    "parcel": "^2.6.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.18.6"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
