{
    "plugins": [
      ["@babel/plugin-transform-react-jsx", {
        "pragma": "MiniFramework.createElement", // default pragma is React.createElement
        "throwIfNamespace": false // defaults to true
      }],
      "faster.js",
      "closure-elimination",
      [
        "groundskeeper-willie",
        {
          "removeConsole": true,
          "removeDebugger": true,
          "removePragma": true
        }
      ]
    ]
  }